  <select id="mfmi" resultMap="myfiles">
  SELECT a.*,e.* ,p.`name` pname,l.jname jname,d.dname dname1
  FROM archives a
  LEFT JOIN employee e
  ON a.emp_fk=e.eid
  LEFT JOIN POSITION p
  ON e.p_fk=p.id
  LEFT JOIN dept d
  ON e.d_fk=d.deptno
  LEFT JOIN LEVEL l
  ON e.l_fk=l.jid
  WHERE e.eid=1
